<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
    <player>
        <regions>
            <region id="header" type="header" styleClass="headerStyle">
                <activities defaultLaunchID="headerMain">
                    <activity id="headerMain" name="header" modelPath="" groupName="" lang="header" class="sim-pictor">
                        <html source="scq-vertical/shell/header/screen.html" />
                        <data source="scq-vertical/shell/header/screen.json" />
                        <style source="scq-vertical/shell/header/screen.css" />
                    </activity>
                </activities>
            </region>
            <region id="mActivity" type="mActivity" edit="false" styleClass="activityStyle">
                <activities defaultLaunchID="question1">
                    <!-- <activity id="start" name="start" modelPath="" modelName="" groupName="" lang="magic/defaultscreen" class="sim-pictor">
                        <style source="mcq/activities/mmcq/start/screen.css" />
                        <html source="mcq/activities/mmcq/start/screen.html" />
                        <data source="mcq/activities/mmcq/start/screen.json" />
                    </activity> -->
                    <activity id="question1" name="question1" modelPath="" modelName="" groupName="" lang="magic/defaultscreen" class="sim-pictor">
                        <style source="scq-vertical/activities/mmcq/question1/screen.css" />
                        <html source="scq-vertical/activities/mmcq/question1/screen.html" />
                        <data source="scq-vertical/activities/mmcq/question1/screen.json" />
                    </activity>
                    <activity id="question2" name="question2" modelPath="" modelName="" groupName="" lang="magic/defaultscreen" class="sim-pictor">
                        <style source="scq-vertical/activities/mmcq/question2/screen.css" />
                        <html source="scq-vertical/activities/mmcq/question2/screen.html" />
                        <data source="scq-vertical/activities/mmcq/question2/screen.json" />
                    </activity>
                    <activity id="question3" name="question3" modelPath="" modelName="" groupName="" lang="magic/defaultscreen" class="sim-pictor">
                        <style source="scq-vertical/activities/mmcq/question3/screen.css" />
                        <html source="scq-vertical/activities/mmcq/question3/screen.html" />
                        <data source="scq-vertical/activities/mmcq/question3/screen.json" />
                    </activity>
                    <activity id="question4" name="question4" modelPath="" modelName="" groupName="" lang="magic/defaultscreen" class="sim-pictor">
                        <style source="scq-vertical/activities/mmcq/question4/screen.css" />
                        <html source="scq-vertical/activities/mmcq/question4/screen.html" />
                        <data source="scq-vertical/activities/mmcq/question4/screen.json" />
                    </activity>
                    <!-- <activity id="question5" name="question5" modelPath="" modelName="" groupName="" lang="magic/defaultscreen" class="sim-pictor">
                        <style source="scq-vertical/activities/mmcq/question5/screen.css" />
                        <html source="scq-vertical/activities/mmcq/question5/screen.html" />
                        <data source="scq-vertical/activities/mmcq/question5/screen.json" />
                    </activity>
                     -->
                </activities>
            </region>
            <region id="footer" type="footer" styleClass="footerStyle ">
                <activities defaultLaunchID="footerMain">
                    <activity id="footerMain" name="footer" modelPath="" groupName="" lang="footer" class="sim-pictor">
                        <html source="scq-vertical/shell/footer/screen.html" />
                        <data source="scq-vertical/shell/footer/screen.json" />
                        <style source="scq-vertical/shell/footer/screen.css" />
                    </activity>
                </activities>
            </region>

        </regions>
        <playerSettings>
            <release>false</release>
            <showPreloader>false</showPreloader>
            <activityPath>
                <keyword>dev</keyword>
                <relativePath>../dev</relativePath>
            </activityPath>
            <header visible="true" />
            <footer visible="true" />
            <debug>
                <mode>false</mode>
                <showTacer>true</showTacer>
                <errorLogger>true</errorLogger>
            </debug>
            <themes defaultTheme="1" prefix="theme">
                <theme id="1">
                    <label>Default Theme</label>
                    <path>scq-vertical/shell/theme/shell.css</path>
                </theme>
            </themes>
            <language>
                <support>false</support>
                <rootPath>mcq/activities/mmcq/localization</rootPath>
                <default>en</default>
            </language>
            <size>
                <align>
                    <horizontal>center</horizontal>
                    <vertical>top</vertical>
                </align>
                <scale maxScale="2" minScale="0" isScalable="true" />
                <width>1024</width>
                <height>768</height>
            </size>
            <webService></webService>
        </playerSettings>
    </player>